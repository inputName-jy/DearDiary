<div>
    {{#each posts as |post|}}
    <div class="cards-containerj">
        <div class="cardj">
            <a href="/post/{{post.id}}">
                <h2>{{post.title}}</h2>
                <p>By: {{post.user.firstName}} {{post.user.lastName}}</p>
            </a>
        </div>
    </div>
    {{/each}}
</div>